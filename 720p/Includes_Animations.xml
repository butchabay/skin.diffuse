<includes>

   <include name="PanelRotate">
		<animation type="WindowOpen" reversible="false" >
			<effect type="rotatey" center="640,0" start="-180" end="0" time="1000" delay="0"/>
			<effect type="fade" start="0" end="100" time="400" delay="600"/>
		</animation>
		<animation type="WindowClose" reversible="false" >
			<effect type="fade" start="100" end="0" time="200" delay="0"/>
		</animation>
	</include>

	<include name="OpenClose1">
		<animation type="WindowOpen">
			<effect type="fade" time="300" center="640,360" start="0" end="300" easing="out" reversible="false" />
		</animation>
		<animation type="WindowClose">
			<effect type="fade" time="400" center="640,360" start="300" end="0" easing="in" reversible="false"/>
		</animation>
	</include>
	
	<include name="transition_Fanart">		
		<animation type="WindowOpen">
			<effect type="slide" time="700" start="0,800"  end="0,0" tween="sine" easing="out" reversible="false"/>
		    <effect type="fade" time="600" start="0" end="100" reversible="false"/>
		</animation>			
		<animation type="WindowClose">
			<effect type="slide" time="500" start="0,0" end="0,800" tween="sine" easing="in" reversible="false" />
			<effect type="fade" time="600" start="100" end="0" />
		</animation>
	</include>
	
	<include name="transition_Fanart2">	
	    <animation effect="fade" start="50" end="100" time="500" reversible="true">WindowOpen</animation>
		<animation effect="fade" start="100" end="0" time="500" reversible="true">WindowClose</animation>
	</include>
	
	<include name="transition_Fanart3">	
	    <animation effect="fade" start="0" end="100" time="800" delay="1000">WindowOpen</animation>
		<animation effect="fade" start="100" end="0" time="100">WindowClose</animation>
	</include>
	
	<include name="transition_Fanart4">	
	    <animation effect="fade" start="100" end="0" time="800" delay="800">WindowClose</animation>
		<animation effect="fade" start="0" end="100" time="800">WindowOpen</animation>
	</include>
	
	<include name="transition_Fanart5">	
	    <animation type="WindowClose">
			<effect type="zoom" time="600" center="auto" start="100" end="110"  tween="sine" easing="in" reversible="false" />
			<effect type="fade" time="600" start="100" end="0" />
		</animation>
		<animation effect="fade" start="0" end="100" time="1000">WindowOpen</animation>
	</include>
	
	<include name="AnimationBreadcrumps">
		<animation effect="slide" end="0,-100" time="2000" tween="cubic" easing="out" delay="500">WindowClose</animation>
		<animation effect="slide" start="0,-100" time="2000" tween="cubic" easing="out" delay="500">WindowOpen</animation>
	</include>
	
	<include name="AnimationLiveTVLeft">
		<animation effect="slide" end="-100,0" time="2000" tween="cubic" easing="out" delay="300">WindowClose</animation>
		<animation effect="slide" start="-100,0" time="2000" tween="cubic" easing="out" delay="500">WindowOpen</animation>
	</include>
	
	<include name="AnimationLiveTVRight">
		<animation effect="slide" end="1300,0" time="2000" tween="cubic" easing="out" delay="300">WindowClose</animation>
		<animation effect="slide" start="1300,0" time="2000" tween="cubic" easing="out" delay="500">WindowOpen</animation>
	</include>
	
	<include name="OpenCloseAnimation">
				<animation type="WindowOpen" reversible="false">
					<effect type="zoom" start="80" end="100" center="640,360" easing="out" tween="back" time="300" />
					<effect type="fade" start="0" end="100" time="300" />
				</animation>
				<animation type="WindowClose" reversible="false">
					<effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="back" time="300" />
					<effect type="fade" start="100" end="0" time="200" />
				</animation>
			</include>

