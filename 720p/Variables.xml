	<variable name="mpaa">
	
	<!-- AUSTRALIA -->
	
		<value condition="substring(listitem.mpaa,Australia:G)">extras/flagging/ratings/australia_g.png</value>
		<value condition="substring(listitem.mpaa,Australia:PG)">extras/flagging/ratings/australia_pg.png</value>
		<value condition="substring(listitem.mpaa,Australia:M) + ! substring(listitem.mpaa,Australia:MA)">extras/flagging/ratings/australia_m.png</value>
		<value condition="substring(listitem.mpaa,Australia:MA)">extras/flagging/ratings/australia_ma.png</value>
		<value condition="substring(listitem.mpaa,Australia:R)">extras/flagging/ratings/australia_r.png</value>
		<value condition="substring(listitem.mpaa,Australia:X)">extras/flagging/ratings/australia_x.png</value>
		
		<!-- US -->
			
		<value condition="substring(listitem.mpaa,Rated G)">extras/flagging/ratings/mpaa_general.png</value>
		<value condition="substring(listitem.mpaa,Rated PG) + !substring(listitem.mpaa,Rated PG-13)">extras/flagging/ratings/mpaa_pg.png</value>
		<value condition="substring(listitem.mpaa,Rated PG-13)">extras/flagging/ratings/mpaa_pg13.png</value>
		<value condition="substring(Listitem.mpaa,Rated R)">extras/flagging/ratings/mpaa_restricted.png</value>
		<value condition="substring(Listitem.mpaa,Rated NC)">extras/flagging/ratings/mpaa_nc17.png</value>
		
		<!-- UK -->
		
		<value condition="substring(listitem.mpaa,UK:U)">extras/flagging/ratings/UK_u.png</value>
		<value condition="substring(listitem.mpaa,UK:PG)">extras/flagging/ratings/UK_pg.png</value>
		<value condition="substring(listitem.mpaa,UK:12a)">extras/flagging/ratings/UK_12a.png</value>
		<value condition="substring(listitem.mpaa,UK:12) + !substring(listitem.mpaa,UK:12a)">extras/flagging/ratings/UK_12.png</value>
		<value condition="substring(listitem.mpaa,UK:15)">extras/flagging/ratings/UK_15.png</value>
		<value condition="substring(listitem.mpaa,UK:18)">extras/flagging/ratings/UK_18.png</value>
		<value condition="substring(listitem.mpaa,UK:R18)">extras/flagging/ratings/UK_r18.png</value>
		
		<!-- GERMANY -->
		
		<value condition="substring(ListItem.mpaa,Germany:o.A.) | substring(ListItem.mpaa, o.A.)">extras/flagging/ratings/Germany0.png</value>
		<value condition="substring(ListItem.mpaa,Germany:6) | substring(ListItem.mpaa,FSK 6)">extras/flagging/ratings/Germany6.png</value>
		<value condition="substring(ListItem.mpaa,Germany:12) | substring(ListItem.mpaa,FSK 12)">extras/flagging/ratings/Germany12.png</value>
		<value condition="substring(ListItem.mpaa,Germany:16) | substring(ListItem.mpaa,FSK 16)">extras/flagging/ratings/Germany16.png</value>
		<value condition="substring(ListItem.mpaa,Germany:18) | substring(ListItem.mpaa,FSK 18)">extras/flagging/ratings/Germany18.png</value>
	</variable>
	
