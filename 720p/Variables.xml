	<variable name="clearcases">
		<value condition="stringcompare(ListItem.VideoResolution,480) + ![stringcompare(ListItem.VideoCodec,divx) | stringcompare(ListItem.VideoCodec,xvid) | stringcompare(ListItem.VideoCodec,dx50) | stringcompare(ListItem.VideoCodec,div3) | stringcompare(ListItem.VideoCodec,div4) | stringcompare(ListItem.VideoCodec,div5) | stringcompare(ListItem.FileExtension,mkv)]">extras/flagging/cases/dvd-clear.png</value>
		<value condition="[stringcompare(ListItem.VideoResolution,720) | stringcompare(ListItem.VideoResolution,1080)] + ![stringcompare(ListItem.VideoResolution,540) | stringcompare(ListItem.FileExtension,mkv) | stringcompare(ListItem.VideoCodec,xvid)]">extras/flagging/cases/bluray-clear.png</value>
		<value condition="[stringcompare(ListItem.VideoResolution,720) | stringcompare(ListItem.VideoResolution,1080) + substring(listitem.filename,3d)] + ![stringcompare(ListItem.VideoResolution,540) | stringcompare(ListItem.FileExtension,mkv) | stringcompare(ListItem.VideoCodec,xvid)]">extras/flagging/cases/3d.png</value>
		<value condition="stringcompare(ListItem.VideoResolution,540) + !stringcompare(ListItem.VideoCodec,xvid)">extras/flagging/cases/hddvd-clear.png</value>
		<value condition="[stringcompare(ListItem.VideoCodec,divx) | stringcompare(ListItem.VideoCodec,dx50) | stringcompare(ListItem.VideoCodec,div3) | stringcompare(ListItem.VideoCodec,div4) | stringcompare(ListItem.VideoCodec,div5)] + !stringcompare(ListItem.VideoResolution,720) + !stringcompare(ListItem.VideoResolution,540)">extras/flagging/cases/divx.png</value>
		<value condition="stringcompare(ListItem.VideoCodec,xvid) + ![stringcompare(ListItem.VideoResolution,720) + stringcompare(ListItem.VideoResolution,540)]">extras/flagging/cases/xvid.png</value>
		<value condition="stringcompare(ListItem.FileExtension,mkv) + ![stringcompare(ListItem.VideoResolution,720) + stringcompare(ListItem.VideoResolution,1080) + stringcompare(ListItem.VideoResolution,540) + stringcompare(ListItem.VideoResolution,480)]">extras/flagging/cases/mkv.png</value>
		<value condition="Library.HasContent(MovieSets) + !Container.Content(TVShows) + !Container.Content(Seasons)">extras/flagging/cases/collection.png</value>
		<value condition="Container.Content(TVShows) | Container.Content(Seasons)">extras/flagging/cases/TVseries.png</value>
	</variable>
	
